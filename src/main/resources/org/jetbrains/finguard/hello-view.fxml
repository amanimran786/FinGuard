<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="org.jetbrains.finguard.HelloController">

    <Label fx:id="welcomeLabel" text="Welcome!" />

    <TextField fx:id="inputField" promptText="Enter your name" />

    <Button fx:id="submitButton" text="Submit" />

</VBox>
